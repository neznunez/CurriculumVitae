{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/chat",
      "dest": "/api/chat.js"
    },
    {
      "src": "/api/get-persona",
      "dest": "/api/get-persona.js"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
